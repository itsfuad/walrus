{
  "name": "walrus",
  "displayName": "Walrus",
  "description": "Walrus Language Support",
  "version": "1.0.15",
  "icon": "icon.png",
  "publisher": "Walrus",
  "activationEvents": [],
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BrainbirdLab/Rex-Language-Support"
  },
  "contributes": {
    "languages": [
      {
        "id": "walrus",
        "aliases": [
          "walrusLang",
          "walrus"
        ],
        "extensions": [
          ".wal",
          ".walrus"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "icon.png",
          "light": "icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "walrus",
        "scopeName": "source.wal",
        "path": "./syntaxes/wal.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "walrus",
        "path": "./snippets/walrus.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Walrus Language Server",
      "properties": {
        "walrusLanguageServer.trace.server": {
          "type": "string",
          "enum": ["off", "messages", "verbose"],
          "default": "off",
          "description": "Traces the communication between VS Code and the Walrus Language Server."
        }
      }
    }
  },
  "main": "./client.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  },
  "keywords": [
    "Walrus",
    "Programming",
    "Language"
  ],
  "author": "Fuad Hasan",
  "license": "ISC"
}
