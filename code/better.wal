
type Person struct {
    name: str,
    age: i32
};

type IPerson interface {
    fn get_name() -> str,
    fn get_age() -> i32
};

let person : Person = Person {
    name: "John",
    age: 25
};

type PersonType Person;

let person2 := PersonType {
    name: "Doe",
    age: 30
};

